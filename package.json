{
  "name": "diconium",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-native": "0.39.2",
    "react-native-swipeout": "^2.0.12"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@ptomasroos|react-clone-referenced-element|realm)"
    ],
    "coverageReporters": [
      "html"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/reports/**",
      "!**/src/Examples/**",
      "!**/config/git-hooks/**",
      "!**/test-data/**/**",
      "!**/index.android.js",
      "!**/index.ios.js",
      "!**/src/App.js"
    ],
    "coverageDirectory": "<rootDir>/reports/coverage/"
  },
  "devDependencies": {
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.7.1",
    "jest": "19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "15.4.2"
  }
}
